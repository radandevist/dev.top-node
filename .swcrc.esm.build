{
  "exclude": [".*\\.spec|test\\.(j|t)s$"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true,
      "topLevelAwait": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es2022",
    "experimental": {
      "keepImportAssertions": true
    }
  },
  "module": {
    "type": "es6"
  },
  "sourceMaps": true
}